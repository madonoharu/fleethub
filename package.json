{
  "name": "fleethub",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "main": "index.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn build:utils && yarn build:data && yarn build:core && yarn build:client",
    "build:client": "yarn workspace @fleethub/client build",
    "build:core": "yarn workspace @fleethub/core build",
    "build:data": "yarn workspace @fleethub/data build",
    "build:utils": "yarn workspace @fleethub/utils build",
    "client": "yarn workspace @fleethub/client",
    "core": "yarn workspace @fleethub/core",
    "data": "yarn workspace @fleethub/data",
    "imagemin": "yarn ts-node scripts/imagemin",
    "lint": "eslint --cache packages && yarn tscheck",
    "start": "yarn client start",
    "test": "yarn test:core",
    "test:core": "jest packages/core/src",
    "tscheck": "tsc --noEmit",
    "utils": "yarn workspace @fleethub/utils"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@fleethub/utils": "*",
    "@types/fs-extra": "^9.0.2",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-webp": "^5.1.1",
    "@types/jest": "^26.0.15",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.14.2",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "axios": "^0.20.0",
    "babel-jest": "^26.6.0",
    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expr-eval": "^2.0.2",
    "firebase-admin": "^9.2.0",
    "google-spreadsheet": "^3.0.13",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "jest": "^26.6.0",
    "kc-tools": "^1.0.1",
    "ky": "^0.24.0",
    "ky-universal": "^0.8.2",
    "lint-staged": "^10.4.2",
    "lodash-es": "^4.17.15",
    "prettier": "^2.1.2",
    "signale": "^1.4.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.0-beta"
  },
  "engines": {
    "npm": "Please use yarn"
  }
}
