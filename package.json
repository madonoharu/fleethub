{
  "name": "fleethub",
  "version": "0.0.10",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn build:utils && yarn build:site",
    "build:core": "sh scripts/build-core.sh",
    "build:site": "yarn workspace @fh/site build",
    "build:utils": "yarn workspace @fh/utils build",
    "dev": "yarn workspace @fh/site dev",
    "dispatch": "yarn ts scripts/dispatch",
    "postinstall": "husky install",
    "lint": "eslint --cache packages && yarn tscheck",
    "site": "yarn workspace @fh/site",
    "start": "yarn workspace @fh/core start",
    "test": "jest",
    "test:fh-macro": "wasm-pack test --node crates/fh-macro",
    "ts": "node -r esbuild-register",
    "tscheck": "tsc --noEmit",
    "update:constants": "yarn ts scripts/updateConstants",
    "update:locales": "yarn ts scripts/updateLocales",
    "update:storage": "yarn ts scripts/updateStorage",
    "utils": "yarn workspace @fh/utils"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write"
  },
  "devDependencies": {
    "@fh/utils": "*",
    "@next/eslint-plugin-next": "^11.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/jest": "^27.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.3",
    "@types/signale": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cloudinary": "^1.27.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.8",
    "esbuild-jest": "^0.5.0",
    "esbuild-node-tsc": "^1.7.0",
    "esbuild-register": "^3.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "firebase-admin": "^10.0.0",
    "fs-extra": "^10.0.0",
    "google-spreadsheet": "^3.1.15",
    "got": "^11.8.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "kc-tools": "^1.1.0",
    "ky": "^0.28.6",
    "lint-staged": "^11.2.3",
    "lodash-es": "^4.17.21",
    "prettier": "^2.4.1",
    "signale": "^1.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "npm": "Please use yarn"
  },
  "packageManager": "yarn@3.0.2"
}
