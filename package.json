{
  "name": "fleethub",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "main": "index.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn build:utils && yarn build:data && yarn build:core && yarn build:client",
    "build:client": "yarn workspace @fleethub/client build",
    "build:core": "yarn workspace @fleethub/core build",
    "build:data": "yarn workspace @fleethub/data build",
    "build:utils": "yarn workspace @fleethub/utils build",
    "client": "yarn workspace @fleethub/client",
    "core": "yarn workspace @fleethub/core",
    "data": "yarn workspace @fleethub/data",
    "imagemin": "yarn tsnode scripts/imagemin",
    "lint": "eslint --cache packages && yarn tscheck",
    "start": "yarn client start",
    "test": "yarn test:core",
    "test:core": "jest packages/core/src",
    "tscheck": "tsc --noEmit",
    "tsnode": "yarn ts-node -O \"{\"\"module\"\":\"\"commonjs\"\"}\"",
    "utils": "yarn workspace @fleethub/utils"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@fleethub/utils": "*",
    "@types/fs-extra": "^9.0.1",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-webp": "^5.1.0",
    "@types/jest": "^26.0.10",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.6.0",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "axios": "^0.20.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "lodash-es": "^4.17.15",
    "prettier": "^2.0.5",
    "signale": "^1.4.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "engines": {
    "npm": "Please use yarn"
  }
}
