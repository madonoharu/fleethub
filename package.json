{
  "name": "fleethub",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "yarn build:utils & yarn build:core",
    "build": "yarn build:utils && yarn build:core && yarn build:site",
    "build:core": "yarn workspace @fleethub/core build",
    "build:site": "yarn workspace @fleethub/site build",
    "build:utils": "yarn workspace @fleethub/utils build",
    "core": "yarn workspace @fleethub/core",
    "dev": "yarn workspace @fleethub/site dev",
    "esn": "node -r esbuild-register",
    "lint": "eslint --cache packages && yarn tscheck",
    "prepare": "husky install",
    "site": "yarn workspace @fleethub/site",
    "test": "jest",
    "tscheck": "tsc --noEmit",
    "update:constants": "yarn esn scripts/updateConstants",
    "update:locales": "yarn esn scripts/updateLocales",
    "utils": "yarn workspace @fleethub/utils"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write"
  },
  "devDependencies": {
    "@fleethub/utils": "*",
    "@next/eslint-plugin-next": "^11.1.1",
    "@types/fs-extra": "^9.0.12",
    "@types/google-spreadsheet": "^3.1.4",
    "@types/jest": "^27.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.7.8",
    "@types/signale": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cloudinary": "^1.26.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.24",
    "esbuild-jest": "^0.5.0",
    "esbuild-node-tsc": "^1.6.1",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "firebase-admin": "^9.11.1",
    "fs-extra": "^10.0.0",
    "google-spreadsheet": "^3.1.15",
    "got": "11.8.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "kc-tools": "^1.1.0",
    "ky": "^0.28.5",
    "lint-staged": "^11.1.2",
    "lodash-es": "^4.17.21",
    "prettier": "^2.3.2",
    "signale": "^1.4.0",
    "typescript": "^4.4.2"
  },
  "engines": {
    "npm": "Please use yarn"
  }
}
