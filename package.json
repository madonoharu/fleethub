{
  "name": "fleethub",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "yarn build:utils & yarn build:core",
    "build": "yarn build:utils && yarn build:data && yarn build:core && yarn build:client",
    "build:client": "yarn workspace @fleethub/client build",
    "build:core": "yarn workspace @fleethub/core build",
    "build:data": "yarn workspace @fleethub/data build",
    "build:utils": "yarn workspace @fleethub/utils build",
    "client": "yarn workspace @fleethub/client",
    "core": "yarn workspace @fleethub/core",
    "data": "yarn workspace @fleethub/data",
    "dev": "yarn workspace @fleethub/client dev",
    "lint": "eslint --cache packages && yarn tscheck",
    "test": "yarn test:core",
    "test:core": "jest packages/core/src",
    "tscheck": "tsc --noEmit",
    "update:locales": "yarn ts-node scripts/updateLocales",
    "update:types": "yarn ts-node scripts/updateTypes",
    "utils": "yarn workspace @fleethub/utils"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@fleethub/utils": "*",
    "@types/fs-extra": "^9.0.5",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/jest": "^26.0.19",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.14",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "axios": "^0.21.0",
    "babel-jest": "^26.6.3",
    "cloudinary": "^1.23.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expr-eval": "^2.0.2",
    "firebase-admin": "^9.4.2",
    "google-spreadsheet": "^3.0.14",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "kc-tools": "^1.1.0",
    "ky": "^0.25.1",
    "ky-universal": "^0.8.2",
    "lint-staged": "^10.5.3",
    "lodash-es": "^4.17.15",
    "prettier": "^2.2.1",
    "signale": "^1.4.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "npm": "Please use yarn"
  }
}
